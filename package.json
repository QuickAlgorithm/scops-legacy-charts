{
  "name": "@quickalgorithm/scops-legacy-charts",
  "version": "0.10.39",
  "description": "Scops Legacy Chart - NVD3",
  "sideEffects": [
    "*.css"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib"
  ],
  "repository": "git://github.com:QuickAlgorithm/scops-legacy-charts.git",
  "keywords": [
    "superset"
  ],
  "scripts": {
    "build": "yarn babel && yarn build:assets",
    "babel": "yarn babel:cjs && yarn babel:esm",
    "babel:cjs": "nimbus babel --clean",
    "babel:esm": "nimbus babel --clean --esm",
    "build:assets": "node ./scripts/buildAssets.js",
    "clean": "rm -rf ./packages/**/{lib,esm}",
    "type": "nimbus typescript --build"
  },
  "author": "Superset",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.6",
    "@data-ui/xy-chart": "^0.0.78",
    "@superset-ui/chart": "^0.11.0",
    "@superset-ui/color": "^0.11.0",
    "@superset-ui/connection": "^0.9.0",
    "@superset-ui/core": "^0.11.0",
    "@superset-ui/dimension": "^0.11.0",
    "@superset-ui/number-format": "^0.11.0",
    "@superset-ui/time-format": "^0.11.0",
    "@superset-ui/translation": "^0.11.0",
    "core-js": "^3.6.5",
    "d3": "^3.5.17",
    "d3-tip": "^0.9.1",
    "dompurify": "^1.0.3",
    "fast-safe-stringify": "^2.0.6",
    "lodash": "^4.17.11",
    "mathjs": "^3.20.2",
    "moment": "^2.20.1",
    "nvd3": "1.8.6",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "urijs": "^1.18.10"
  },
  "devDependencies": {
    "@airbnb/config-babel": "^2.1.3",
    "@airbnb/config-eslint": "^2.1.3",
    "@airbnb/config-jest": "^2.1.3",
    "@airbnb/config-prettier": "^2.0.4",
    "@airbnb/config-typescript": "^2.1.2",
    "@airbnb/nimbus": "^2.1.3",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@superset-ui/commit-config": "^0.0.9",
    "@superset-ui/superset-ui": "^0.12.5",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/react": "^16.9.0",
    "@types/react-test-renderer": "^16.9.0",
    "csstype": "^2.6.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "fast-glob": "^3.0.1",
    "fs-extra": "^8.0.1",
    "husky": "^3.0.1",
    "jest-mock-console": "^1.0.0",
    "lerna": "^3.15.0",
    "lint-staged": "^9.2.5",
    "react-test-renderer": "^16.9.0"
  },
  "peerDependencies": {
    "@superset-ui/chart": "^0.10.0 || ^0.11.0",
    "@superset-ui/color": "^0.10.0 || ^0.11.0",
    "@superset-ui/core": "^0.10.0 || ^0.11.0",
    "@superset-ui/dimension": "^0.10.0 || ^0.11.0",
    "@superset-ui/number-format": "^0.10.0 || ^0.11.0",
    "@superset-ui/time-format": "^0.10.0 || ^0.11.0",
    "@superset-ui/translation": "^0.10.0 || ^0.11.0",
    "react": "^15 || ^16"
  },
  "beemo": {
    "module": "@superset-ui/build-config",
    "drivers": [
      "babel",
      "eslint",
      {
        "driver": "jest",
        "env": {
          "NODE_ENV": "test"
        }
      },
      "prettier",
      "typescript"
    ]
  },
  "engines": {
    "node": ">=10.10.0",
    "npm": ">=6.8.0",
    "yarn": ">=1.13.0"
  },
  "nimbus": {
    "drivers": [
      "babel",
      "eslint",
      "jest",
      "prettier",
      "typescript"
    ],
    "settings": {
      "library": true,
      "react": true,
      "next": true,
      "env": {
        "corejs": 3,
        "useBuiltIns": "usage"
      }
    },
    "babel": {
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3
          }
        ]
      ]
    },
    "jest": {
      "timers": "real",
      "setupFilesAfterEnv": [
        "@airbnb/config-jest/enzyme"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 1,
          "functions": 1,
          "lines": 1,
          "statements": 1
        }
      }
    },
    "eslint": {
      "overrides": [
        {
          "files": "*.test.{js,jsx,ts,tsx}",
          "rules": {
            "import/no-extraneous-dependencies": "off",
            "promise/param-names": "off",
            "import/extensions": "off",
            "jest/require-to-throw-message": "off",
            "jest/no-test-return-statement": "off",
            "jest/no-expect-resolves": "off",
            "jest/no-test-callback": "off"
          }
        },
        {
          "files": "*.{js,jsx,ts,tsx}",
          "rules": {
            "react/jsx-no-literals": "off",
            "prefer-exponentiation-operator": "off",
            "@typescript-eslint/no-explicit-any": [
              "warn",
              {
                "fixToUnknown": false
              }
            ]
          }
        }
      ]
    },
    "typescript": {
      "compilerOptions": {
        "emitDeclarationOnly": true
      }
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
